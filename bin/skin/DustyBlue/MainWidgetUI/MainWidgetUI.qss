QWidget#mainBg{
    border-image: url(skin/DustyBlue/MainWidgetUI/main/bg.png);
}

QPushButton#btnQuit{
    outline: none;
    background-color: transparent;
    background-position: center;
    background-repeat: none;
	border-image: url(skin/DustyBlue/MainWidgetUI/main/winclose-n.png);
}
QPushButton#btnQuit:hover{
	border-image: url(skin/DustyBlue/MainWidgetUI/main/winclose-h.png);
}
QPushButton#btnQuit:pressed{
	border-image: url(skin/DustyBlue/MainWidgetUI/main/winclose-p.png);
}
QPushButton#btnQuit:disabled{
	border-image: url(skin/DustyBlue/MainWidgetUI/main/winclose-d.png);
}
